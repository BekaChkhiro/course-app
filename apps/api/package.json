{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate --schema=../../packages/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=../../packages/database/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=../../packages/database/prisma/schema.prisma",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@types/database": "*",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/ioredis": "^4.28.10",
    "@types/types": "*",
    "@types/utils": "*",
    "@videojs/http-streaming": "^3.17.2",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "resend": "^6.5.2",
    "stripe": "^14.9.0",
    "ua-parser-js": "^2.0.6",
    "video.js": "^8.23.4",
    "videojs-contrib-quality-levels": "^4.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.5",
    "@types/ua-parser-js": "^0.7.39",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
